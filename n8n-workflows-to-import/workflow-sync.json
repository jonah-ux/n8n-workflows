      "type": "n8n-nodes-base.set",
      "typeVersion": 3.4,
      "position": [680, 300]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "INSERT INTO workflows (workflow_id, name, description, category, workflow_json, node_count, is_active, source)\nVALUES ($1, $2, $3, 'personal', $4::jsonb, $5, $6, 'user')\nON CONFLICT (workflow_id) DO UPDATE SET\n  name = EXCLUDED.name,\n  description = EXCLUDED.description,\n  workflow_json = EXCLUDED.workflow_json,\n  node_count = EXCLUDED.node_count,\n  is_active = EXCLUDED.is_active,\n  updated_at = NOW()",
        "additionalFields": {
          "queryParameters": {
            "parameters": [
              {"name": "workflow_id", "value": "={{ $json.workflow_id }}"},
              {"name": "name", "value": "={{ $json.name }}"},
              {"name": "description", "value": "={{ $json.description }}"},
              {"name": "workflow_json", "value": "={{ $json.workflow_json }}"},
              {"name": "node_count", "value": "={{ $json.node_count }}"},
              {"name": "is_active", "value": "={{ $json.is_active }}"}
            ]
          }
        }
      },
      "id": "4",
      "name": "Save to Database",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.5,
      "position": [900, 300],
