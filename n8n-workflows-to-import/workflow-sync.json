        "n8nApi": {
          "id": "1",
          "name": "n8n API"
        }
      }
    },
    {
      "parameters": {
        "assignments": {
          "assignments": [
            {"id": "workflow_id", "name": "workflow_id", "value": "={{ $json.id }}", "type": "string"},
            {"id": "name", "name": "name", "value": "={{ $json.name }}", "type": "string"},
            {"id": "description", "name": "description", "value": "={{ 'Workflow with ' + ($json.nodes?.length || 0) + ' nodes' }}", "type": "string"},
